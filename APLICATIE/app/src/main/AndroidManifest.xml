<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.infotrip">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/louncher2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/louncher2_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UserProfileActivity"></activity>
        <activity android:name=".PrincipalMeniu" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogInActivity" />
        <activity android:name=".SingUpActivity" />
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>